{
  "name": "JSECoinServer",
  "version": "1.4.0",
  "description": "JSE Coin Server",
  "main": "server.js",
  "scripts": {
    "test": "mocha --delay --timeout 9999999 test/tests.js",
    "start": "node server.js",
    "server": "forever start -c \"node --max-old-space-size=3000\" server.js -b -t remote -s server.jsecoin.com -m 0 &",
    "load": "forever start -c \"node --max-old-space-size=3000\" server.js -b -t remote -s load.jsecoin.com -m 0 &",
    "db": "forever start -c \"node --max-old-space-size=3000\" db.js &",
    "controller": "forever start -c \"node --max-old-space-size=3000\" controller.js &",
    "docs": "jsdoc -c .jsdocsconf.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs": "^0.13.5",
    "@tensorflow/tfjs-node": "^0.1.21",
    "authenticator": "^1.1.5",
    "body-parser": "^1.19.0",
    "build-tools": "^7.3.0",
    "commander": "^2.20.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "eccrypto": "^1.1.3",
    "express": "^4.17.1",
    "express-rate-limit": "^2.14.2",
    "forever": "^0.15.2",
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "jimp": "^0.6.8",
    "jsdoc": "^3.6.3",
    "maxmind": "^2.12.0",
    "mocha": "^5.2.0",
    "multer": "^1.4.2",
    "puppeteer": "^1.20.0",
    "request": "^2.88.2",
    "secure-random": "^1.1.2",
    "sendgrid": "^5.2.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "twilio": "^3.39.5",
    "uglifyjs": "^2.4.11",
    "web3": "^1.2.6"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
